Private Sub Command27_Click()
' Save Changes To Table & Close The Form
    On Error GoTo Err_cmdCloseForm_Click
    DoCmd.RunCommand acCmdSaveRecord
   
    
        ' Save the calculate Expiry date into the due by date via an update query
        Dim strSQL As String
        strSQL = "UPDATE tbl3curr_staff_MCourseAlloc SET DueDate = #" & Me.Date_CourseExpiry & "# WHERE FK_StaffID = " & Me.FK_StaffID.Value & " AND FK_MCourseID = " & Me.FK_MCourseID.Value
        DoCmd.RunSQL strSQL
        'DoCmd.Requery
    
     DoCmd.Close
    

Exit_cmdCloseForm_Click:
    Exit Sub

Err_cmdCloseForm_Click:
    MsgBox Err.Description
    Resume Exit_cmdCloseForm_Click







End Sub

Private Sub Date_CourseCompletion_AfterUpdate()
    Dim exp As Variant
    exp = DLookup("[Validity_months]", "tbl1_MCourseCatalog", "[ID_MCourseCat] = " & Me.FK_MCourseID.Value)
    Me.Date_CourseExpiry.Value = DateAdd("m", exp, Me.Date_CourseCompletion.Value)
 
 
 
 
 
End Sub

